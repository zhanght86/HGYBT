/* ---------------------------------------------------- */
/* cooDatePicker                    */
/* ---------------------------------------------------- */
.coolDatePicker
{
	behavior: url(../common/behaviors/coolDatePicker.htc);
	buttonImageUrl: ../common/images/calendar.gif;
	prevImageUrl: ../common/images/prev.gif;
	nextImageUrl: ../common/images/next.gif;
	font-size: 10pt;
	font-family: Verdana, Arial, Helvetica;
	vertical-align: top;
	margin: 0px;
	border: 1px #799AE1 solid;
	padding: 1px;
	height: 20px;
	width: 160px;
	background-color: #F7F7F7;
}
.coolDatePickerLeftHeader,
.coolDatePickerCenterHeader,
.coolDatePickerRightHeader,
.coolDatePickerDay,
.coolDatePickerDate
{
/*	background-color: buttonface;*/
	background-color: #F7F7F7;
	border-right: buttonshadow 1px solid;
	border-top: buttonhighlight 1px solid;
	border-left: buttonhighlight 1px solid;
	border-bottom: buttonshadow 1px solid;
	position: absolute;
	text-align: center;
	font: bolder 8pt Verdana;
	height: 18px;
	width: 19px;
}
.coolDatePickerLeftHeader
{
	left: 0px;
	cursor: hand;
}
.coolDatePickerCenterHeader
{
	padding-top: 1px;
	width: 95px;
	left: 19px;
	cursor: default;
}
.coolDatePickerRightHeader
{
	left: 114px;
	cursor: hand;
}
.coolDatePickerDay
{
	padding-top: 1px;
	top: 18px;
	cursor: default;
}
.coolDatePickerDate
{
	padding-top: 2px;
	font: 7.5pt Verdana;
	cursor: hand;
}
